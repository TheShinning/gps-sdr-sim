    "version": "0.2.0",
    "configurations": [
        {
            "name": "debug pppar without build",
            "type": "cppdbg",
            "request": "launch",
            "program": "H:/winmove/project/pppar/pppartest/GARPS/build/Bin/pppar.exe",
            "args": [
                "-C",
                "H:/winmove/project/pppar/pppar_main/conf/PPP/ppp_mgex_2021_3f.conf",
                "-S",
                "GEC",
                "-M",
                "PPP-KINE",
                "-A",
                "0",
                "-L",
                "0"
            ],
            "stopAtEntry": false,
            "cwd": "H:/winmove/project/pppar/pppartest/GARPS/build/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}